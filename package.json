    "lodash.merge": "^4.6.2",
    "lorem-ipsum": "^2.0.3",
    "normalize.css": "^8.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-textarea-autosize": "^7.1.2",
